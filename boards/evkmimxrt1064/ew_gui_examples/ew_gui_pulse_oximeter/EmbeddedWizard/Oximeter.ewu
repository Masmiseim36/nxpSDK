    if (( i < 0 ) || ( i >= NoOfData ))
      return 0.0;
    else
      return Data[ i ];

  }

  $rect <20,270,220,310>
  method float GetSlope( arg int32 aIndex )
  {
    if (( aIndex < 0 ) || ( aIndex > NoOfData - 1 ))
      return 0.0;
    else
