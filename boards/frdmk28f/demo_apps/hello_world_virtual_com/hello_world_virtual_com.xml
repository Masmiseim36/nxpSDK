      <files mask="clock_config.c"/>
    </source>
    <source path="boards/frdmk28f/demo_apps/hello_world_virtual_com" target_path="doc" type="doc" toolchain="iar mdk mcuxpresso">
      <files mask="readme.txt"/>
    </source>
    <source path="boards/frdmk28f/demo_apps/hello_world_virtual_com" target_path="source" type="src">
      <files mask="hello_world.c"/>
    </source>
    <source path="devices/MK28F15/utilities" target_path="utilities" type="c_include">
