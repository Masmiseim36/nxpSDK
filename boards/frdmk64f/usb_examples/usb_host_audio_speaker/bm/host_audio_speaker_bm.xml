  <example id="frdmk64f_host_audio_speaker_bm" name="host_audio_speaker_bm" category="usb_examples" dependency="middleware.usb.host_controller_khci.MK64F12 middleware.baremetal.MK64F12 platform.drivers.port.MK64F12 platform.drivers.common.MK64F12 platform.drivers.clock.MK64F12 platform.drivers.sim.MK64F12 middleware.usb.host_audio.MK64F12 middleware.usb.host_hid.MK64F12 middleware.usb.host_hub.MK64F12 platform.drivers.gpio.MK64F12 platform.drivers.flash.MK64F12 platform.drivers.sysmpu.MK64F12 platform.utilities.debug_console.MK64F12 platform.drivers.uart.MK64F12 platform.drivers.smc.MK64F12 platform.devices.MK64F12_CMSIS.MK64F12 platform.devices.MK64F12_startup.MK64F12 platform.utilities.assert.MK64F12" toolchain="iar mdk armgcc kds mcuxpresso">
    <projects>
      <project type="com.crt.advproject.projecttype.exe" nature="org.eclipse.cdt.core.cnature"/>
    </projects>
    <toolchainSettings>
      <!-- Curently, mcux just supports one common settings set for both debug and release -->
      <toolchainSetting id_refs="com.nxp.mcuxpresso">
        <!-- asm compiler -->
        <!-- gcc compiler -->
        <option id="com.crt.advproject.c.misc.dialect" type="enum">
          <value>com.crt.advproject.misc.dialect.gnu99</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.debug.option.debugging.level" type="enum">
          <value>gnu.c.debugging.level.default</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.release.option.debugging.level" type="enum">
          <value>gnu.c.debugging.level.default</value>
        </option>
        <option id="gnu.c.compiler.option.preprocessor.def.symbols" type="stringList">
          <value>_DEBUG=1</value>
          <value>CPU_MK64FN1M0VLL12</value>
          <value>USB_STACK_BM</value>
          <value>FRDM_K64F</value>
          <value>FREEDOM</value>
        </option>
        <option id="gnu.c.compiler.option.optimization.flags" type="string">
          <value>-fno-common</value>
        </option>
        <option id="gnu.c.compiler.option.misc.other" type="string">
          <value>-c  -ffunction-sections  -fdata-sections  -ffreestanding  -fno-builtin</value>
        </option>
        <!-- gcc linker -->
        <option id="gnu.c.link.option.nostdlibs" type="boolean">
          <value>true</value>
        </option>
        <option id="gnu.c.link.option.other" type="stringList">
          <value>--gc-sections</value>
        </option>
        <option id="com.crt.advproject.link.memory.heapAndStack" type="string">
          <value>&amp;Heap:Default;Default;0x2000&amp;Stack:Default;Default;0x2000</value>
        </option>
      </toolchainSetting>
    </toolchainSettings>
