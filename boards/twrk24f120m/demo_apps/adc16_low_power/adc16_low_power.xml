  <example id="twrk24f120m_adc16_low_power" name="adc16_low_power" category="demo_apps" dependency="platform.drivers.clock.MK24F25612 platform.drivers.flash.MK24F25612 platform.drivers.common.MK24F25612 platform.devices.MK24F25612_CMSIS.MK24F25612 platform.devices.MK24F25612_startup.MK24F25612 platform.drivers.uart.MK24F25612 platform.drivers.port.MK24F25612 platform.drivers.smc.MK24F25612 platform.utilities.debug_console.MK24F25612 platform.drivers.gpio.MK24F25612 platform.utilities.assert.MK24F25612 platform.drivers.lptmr.MK24F25612 platform.drivers.adc16.MK24F25612 platform.drivers.pmc.MK24F25612" toolchain="iar mdk armgcc kds mcuxpresso">
    <projects>
      <project type="com.crt.advproject.projecttype.exe" nature="org.eclipse.cdt.core.cnature"/>
    </projects>
    <toolchainSettings>
      <!-- Curently, mcux just supports one common settings set for both debug and release -->
      <toolchainSetting id_refs="com.nxp.mcuxpresso">
        <!-- asm compiler -->
        <!-- gcc compiler -->
        <option id="com.crt.advproject.c.misc.dialect" type="enum">
          <value>com.crt.advproject.misc.dialect.gnu99</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.debug.option.debugging.level" type="enum">
          <value>gnu.c.debugging.level.default</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.release.option.debugging.level" type="enum">
          <value>gnu.c.debugging.level.default</value>
        </option>
        <option id="gnu.c.compiler.option.preprocessor.def.symbols" type="stringList">
          <value>CPU_MK24FN256VDC12</value>
          <value>TWR_K24F120M</value>
          <value>TOWER</value>
        </option>
        <option id="gnu.c.compiler.option.optimization.flags" type="string">
          <value>-fno-common</value>
        </option>
        <option id="gnu.c.compiler.option.misc.other" type="string">
          <value>-c  -ffunction-sections  -fdata-sections  -ffreestanding  -fno-builtin</value>
        </option>
        <!-- gcc linker -->
        <option id="gnu.c.link.option.nostdlibs" type="boolean">
          <value>true</value>
        </option>
        <option id="gnu.c.link.option.other" type="stringList">
          <value>--gc-sections</value>
        </option>
      </toolchainSetting>
    </toolchainSettings>
    <source path="boards/twrk24f120m/demo_apps/adc16_low_power" target_path="board" type="c_include">
      <files mask="board.h"/>
      <files mask="clock_config.h"/>
      <files mask="pin_mux.h"/>
    </source>
    <source path="boards/twrk24f120m/demo_apps/adc16_low_power" target_path="board" type="src">
