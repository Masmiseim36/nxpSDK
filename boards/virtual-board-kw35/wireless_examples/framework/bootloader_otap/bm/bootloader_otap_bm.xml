  <example id="virtual-board-kw35_wireless_examples_framework_bootloader_otap_bm" name="bm" category="wireless_examples/framework/bootloader_otap" dependency="middleware.wireless.framework.function_lib.MKW35Z4 platform.drivers.common.MKW35Z4 platform.devices.MKW35Z4_CMSIS.MKW35Z4 platform.drivers.flash.MKW35Z4 platform.drivers.lpuart.MKW35Z4 platform.drivers.gpio.MKW35Z4 platform.drivers.port.MKW35Z4 middleware.wireless.framework.common.MKW35Z4 platform.drivers.rtc.MKW35Z4 platform.drivers.smc.MKW35Z4 platform.drivers.clock.MKW35Z4 platform.utilities.debug_console.MKW35Z4" toolchain="iar mcuxpresso drt">
    <projects>
      <project type="com.crt.advproject.projecttype.exe" nature="org.eclipse.cdt.core.cnature"/>
    </projects>
    <memory>
      <memoryBlock addr="00000000" size="00004000" access="RO" type="Flash" id="PROGRAM_FLASH_" name="PROGRAM_FLASH"/>
      <memoryBlock addr="1FFFC0C0" size="0000FF40" access="RW" type="RAM" id="SRAM_" name="SRAM"/>
      <memoryBlock addr="1FFFC000" size="000000C0" access="RW" type="RAM" id="RAM_VECTOR_TABLE_" name="RAM_VECTOR_TABLE"/>
    </memory>
    <toolchainSettings>
      <!-- Curently, mcux just supports one common settings set for both debug and release -->
      <toolchainSetting id_refs="com.nxp.mcuxpresso">
        <!-- asm compiler -->
        <option id="com.crt.advproject.gas.fpu" type="enum">
          <value>com.crt.advproject.gas.fpu.none</value>
        </option>
        <option id="gnu.both.asm.option.warnings.nowarn" type="boolean">
          <value>false</value>
        </option>
        <!-- gcc compiler -->
        <option id="com.crt.advproject.gcc.fpu" type="string">
          <value>com.crt.advproject.gcc.fpu.none</value>
        </option>
        <option id="com.crt.advproject.c.misc.dialect" type="enum">
          <value>com.crt.advproject.misc.dialect.gnu99</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.debug.option.debugging.level" type="enum">
          <value>gnu.c.debugging.level.default</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.release.option.debugging.level" type="enum">
          <value>gnu.c.debugging.level.default</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.debug.option.optimization.level" type="enum">
          <value>gnu.c.optimization.level.optimize</value>
        </option>
        <option id="com.crt.advproject.gcc.exe.release.option.optimization.level" type="enum">
          <value>gnu.c.optimization.level.optimize</value>
        </option>
        <option id="gnu.c.compiler.option.warnings.nowarn" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.compiler.option.warnings.allwarn" type="boolean">
          <value>true</value>
        </option>
        <option id="gnu.c.compiler.option.warnings.extrawarn" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.compiler.option.warnings.wconversion" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.compiler.option.warnings.toerrors" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.compiler.option.preprocessor.def.symbols" type="stringList">
          <value>DEBUG</value>
          <value>__SEMIHOST_HARDFAULT_DISABLE=1</value>
          <value>CPU_MKW35Z512VHT4</value>
          <value>gEepromType_d=gEepromDevice_AT45DB041E_c</value>
          <value>gUseOTAPBootloader_d=1</value>
        </option>
        <option id="gnu.c.compiler.option.optimization.flags" type="string">
          <value>-fno-common</value>
        </option>
        <option id="gnu.c.compiler.option.misc.other" type="string">
          <value>-c  -ffunction-sections  -fdata-sections  -ffreestanding  -fno-builtin -imacros "${ProjDirPath}/source/app_preinclude.h"</value>
        </option>
        <option id="gnu.c.compiler.option.preprocessor.nostdinc" type="boolean">
          <value>false</value>
        </option>
        <!-- gcc linker -->
        <option id="com.crt.advproject.link.fpu" type="enum">
          <value>com.crt.advproject.link.fpu.none</value>
        </option>
        <option id="gnu.c.link.option.nostart" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.link.option.nodeflibs" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.link.option.nostdlibs" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.link.option.strip" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.link.option.noshared" type="boolean">
          <value>false</value>
        </option>
        <option id="gnu.c.link.option.other" type="stringList">
          <value>--gc-sections</value>
          <value>--defsym=__region_BOOT_ROM_end__=0x00003FFF</value>
          <value>--defsym=__SP_INIT=__top_SRAM-8</value>
        </option>
        <option id="gnu.c.link.option.libs" type="stringList">
          <value>m</value>
          <value>c</value>
          <value>gcc</value>
          <value>nosys</value>
        </option>
      </toolchainSetting>
    </toolchainSettings>
    <debug_configurations>
      <debug_configuration id_refs="com.crt.advproject.config.exe.debug com.crt.advproject.config.exe.release">
        <drivers>
          <driver id_refs="PROGRAM_FLASH_">
            <driverBinary path="." type="binary">
              <files mask="FTFE_2K_PD.cfx"/>
            </driverBinary>
          </driver>
        </drivers>
      </debug_configuration>
    </debug_configurations>
